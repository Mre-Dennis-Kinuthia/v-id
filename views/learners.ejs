<!-- views/learners.ejs -->

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>List of Learners</title>
</head>

<body>
    <h1>List of Learners</h1>
    <ul>
        <% learners.forEach((learner)=> { %>
            <li>
                <%= learner.Name %> - <%= learner.learnerEmail %> - <%= learner.Program %>
            </li>
            <% }); %>
    </ul>
</body>

</html>